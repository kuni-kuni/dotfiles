nnoremap j 4j
nnoremap k 4k
nnoremap J 4h
nnoremap K 4l
nnoremap h H
nnoremap l L
nnoremap H gT
nnoremap L gt

set hintchars=asdjkl

" 入力フォームにフォーカスしない
set focuscontent

" tabopenでサーチエンジンのみ
set complete=s

" commandモードでIMEオフ
style -name=commandline-ime chrome://* #liberator-commandline-command input {ime-mode: inactive;}
